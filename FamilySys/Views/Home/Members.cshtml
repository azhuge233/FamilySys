@using FamilySys.Models.DbModels
@model Sakura.AspNetCore.IPagedList<FamilySys.Models.DbModels.User>
@{
    ViewData["Title"] = "访客";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>家庭成员</h2>

<table class="table">
	<tr>
		<th style="text-align: center">ID</th>
		<th style="text-align: center">姓名</th>
		<th style="text-align: center">性别</th>
		<th style="text-align: center">电话</th>
		<th style="text-align: center">邮箱</th>
		<th style="text-align: center">积分</th>
	</tr>
	@foreach (var each in Model)
	{
		<tr>
			<td style="text-align: center">@each.ID</td>
			<td style="text-align: center">@each.Username</td>
			<td style="text-align: center">@each.Sex</td>
			<td style="text-align: center">@each.Phone</td>
			<td style="text-align: center">@each.Mail</td>
			<td style="text-align: center">@each.Score</td>
		</tr>
	}
</table>
<div align="center">
	<pager source="Model"/>
</div>

